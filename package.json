{
  "name": "la-discotheque",
  "version": "1.0.1",
  "description": "Fancy Discogs-powered collection viewer",
  "author": "lovethebomb <lucas@lovethebomb.eu>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once --pro"
  },
  "dependencies": {
    "@babel/preset-env": "^7.5.0",
    "apicache": "^1.2.3",
    "core-js": "^2.6.4",
    "cross-env": "^5.2.0",
    "debounce": "^1.2.0",
    "dotenv": "^7.0.0",
    "gsap": "^2.0.2",
    "isomorphic-fetch": "^2.2.1",
    "nuxt": "^2.4.3",
    "postcss-nested": "^4.1.1",
    "slugify": "^1.3.4"
  },
  "devDependencies": {
    "@babel/runtime": "^7.3.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-vue": "^5.1.0",
    "jest": "^24.1.0",
    "jest-serializer-vue": "^2.0.2",
    "nodemon": "^1.18.10",
    "prettier": "1.16.4",
    "semantic-release": "^15.13.3",
    "travis-deploy-once": "^5.0.11",
    "vue-jest": "^3.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.vue$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|vue|json)$",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lovethebomb/la-discotheque.git"
  }
}
